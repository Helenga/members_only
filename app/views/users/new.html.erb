<%= form_for @user do |f| %>
	<%= f.label :name %><br>
	<%= f.text_field :name %><br>
	
	<%= f.label :email %><br>
	<%= f.email_field :email %><br>
	
	<%= f.label :password %><br>
	<%= f.password_field :password %><br>
	
	<%= f.label :password_confirmation %><br>
	<%= f.password_field :password_confirmation %><br>
	
	<%= f.check_box :prime_key %>
	<%= f.label 'Especially invited' %><br>
	
	<%= f.submit 'Sign up' %>
<% end %>
